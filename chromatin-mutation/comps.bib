	langid = {english},
	keywords = {Computational biology and bioinformatics, {DNA} sequencing},
	file = {Full Text PDF:/home/adam/Zotero/storage/6GXLEA6V/Szalay and Golovchenko - 2015 - De novo sequencing and variant calling with nanopo.pdf:application/pdf;Snapshot:/home/adam/Zotero/storage/AAEPZSR3/nbt.3360.html:text/html}
}

@article{norris_nanopore_2016,
	title = {Nanopore sequencing detects structural variants in cancer},
	volume = {17},
	issn = {1538-4047},
	url = {http://dx.doi.org/10.1080/15384047.2016.1139236},
	doi = {10.1080/15384047.2016.1139236},
	abstract = {Despite advances in sequencing, structural variants ({SVs}) remain difficult to reliably detect due to the short read length ({\textless}300 bp) of 2nd generation sequencing. Not only do the reads (or paired-end reads) need to straddle a breakpoint, but repetitive elements often lead to ambiguities in the alignment of short reads. We propose to use the long-reads (up to 20 kb) possible with 3rd generation sequencing, specifically nanopore sequencing on the {MinION}. Nanopore sequencing relies on a similar concept to a Coulter counter, reading the {DNA} sequence from the change in electrical current resulting from a {DNA} strand being forced through a nanometer-sized pore embedded in a membrane. Though nanopore sequencing currently has a relatively high mismatch rate that precludes base substitution and small frameshift mutation detection, its accuracy is sufficient for {SV} detection because of its long reads. In fact, long reads in some cases may improve {SV} detection efficiency.We have tested nanopore sequencing to detect a series of well-characterized {SVs}, including large deletions, inversions, and translocations that inactivate the {CDKN}2A/p16 and {SMAD}4/{DPC}4 tumor suppressor genes in pancreatic cancer. Using {PCR} amplicon mixes, we have demonstrated that nanopore sequencing can detect large deletions, translocations and inversions at dilutions as low as 1:100, with as few as 500 reads per sample. Given the speed, small footprint, and low capital cost, nanopore sequencing could become the ideal tool for the low-level detection of cancer-associated {SVs} needed for molecular relapse, early detection, or therapeutic monitoring.},
	pages = {246--253},
	number = {3},
	journaltitle = {Cancer Biology \& Therapy},
	author = {Norris, Alexis L. and Workman, Rachael E. and Fan, Yunfan and Eshleman, James R. and Timp, Winston},
	urldate = {2017-10-24},
	date = {2016-03-03},
	pmid = {26787508},
	keywords = {{DNA} sequencing, 3rd generation sequencing, cancer diagnostics, Deletions, inversions, nanopore sequencing, next generation sequencing, structural variation, translocations, tumor suppressor gene},
	file = {Full Text PDF:/home/adam/Zotero/storage/XA4NW7DR/Norris et al. - 2016 - Nanopore sequencing detects structural variants in.pdf:application/pdf;Snapshot:/home/adam/Zotero/storage/VAKBX82N/15384047.2016.html:text/html}
}

